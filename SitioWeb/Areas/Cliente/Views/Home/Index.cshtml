                                            @for (int i = 0; i < lista.Count; i++)
                                            {
                                                <option value="@lista[i].IdUsoProp">@lista[i].Descripcion </option>
                                            }
                                        </select>
                                    }
                                </div>
                                <input type="text" name="buscar" id="busquedaVenta" placeholder="Busca un lugar o ciudad">
                                <input type="hidden" name="intencion" value="Venta" id="intencionVenta">
                                <button type="submit" class="thm-btn banner_one_search_btn">
                                    @localizer["Texto5"]
                                </button>

                            </div>
                        }
                    </div>

                    <div class="tab" id="addi__info">
                        @using (Html.BeginForm("BusquedaPropiedades", "Home"))
                        {
                            <div class="banner_one_search_form_input_box">
                                <div class="main-nav__navigation-box">

                                    @{
                                        //string intencion = "venta";
                                        var listaVenta = this.contenedorTrabajo.HomeCliente.ObtenerListaUsosPropiedadesVenta();

                                        //var lista = this.contenedorTrabajo.HomeCliente.Test(intencion);
                                        <select class="myButton" id="seleccionado" name="seleccionado">
                                            <option value="01" selected="selected">TODAS</option>
                                            @for (int i = 0; i < listaVenta.Count; i++)
                                            {
                                                <option value="@listaVenta[i].IdUsoProp">@listaVenta[i].Descripcion </option>
                                            }
                                        </select>
                                    }
                                </div>
                                <input type="text" name="buscar" id="busquedaRenta" placeholder="Busca un lugar o ciudad">
                                <input type="hidden" name="intencion" value="Renta" id="intencionRenta">
                                <button type="submit" class="thm-btn banner_one_search_btn">
                                    @localizer["Texto5"]
                                </button>

                            </div>
                        }
                    </div>
                </div>
            </div>
            <div class="row banner_one_bottom_icon_text">
                <div class="banner_one_bottom_icon">
                    <span class="icon-building"></span>
                </div>
                <div class="banner_one_bottom_text">
                    <p>@localizer["Texto6"]</p>
                </div>
            </div>
            <div class="banner_one_bottom_iconos">
                <div class="header_top_one_content_box_bottom__social float-right">

                    <a href="https://www.facebook.com/Sol-y-Luna-Real-Estate-CR-1820981024597878" style="background-color:blue;color:white"><i class="fab fa-facebook-square"></i></a>

                    <a href="https://api.whatsapp.com/send?phone=50687289082" style="background-color:green;color:white"><i class="fab fa-whatsapp"></i></a>
                </div>
            </div>

        </div>
    </div>
</section>
