         data-options='{"delay": 5000, "slides": [ {"src": "/ContentCliente/assets/images/main-slider/banner-slider-1.jpg"}, {"src":"/ContentCliente/assets/images/main-slider/banner-slider-2.jpg" }, {"src":"/ContentCliente/assets/images/main-slider/banner-slider-3.jpg" }, {"src":"/ContentCliente/assets/images/main-slider/banner-slider-4.jpg" }, {"src":"/ContentCliente/assets/images/main-slider/banner-slider-5.jpg" } ], "transition": "fade", "timer": false, "align": "top", "animation": ["kenburnsUp", "kenburnsDown", "kenburnsLeft", "kenburnsRight" ] }'>
    </div><!-- /.banner-bg-slide -->
    <div class="container">
        <div class="content-box" style="margin-top:-15%">
