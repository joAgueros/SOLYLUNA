                                <span class="info-box-icon bg-danger"><i class="fas fa-money-bill"></i></span>

                                <div class="info-box-content">
                                    <span class="info-box-number">Precio máximo</span>
                                    @if (Model.Moneda != "Colones")
                                    {
                                        <span class="info-box-text"><span>$</span> @precioMaxFormat</span>
                                    }
                                    else
                                    {

                                        <span class="info-box-text"><span>₡</span> @precioMaxFormat</span>
                                    }

                                </div>
                                <!-- /.info-box-content -->
                            </div>
                            <!-- /.info-box -->
                        </div>
                        <div class="col-md-3 col-sm-6 col-12">
                            <div class="info-box">
                                <span class="info-box-icon bg-danger"><i class="fas fa-money-bill"></i></span>

                                <div class="info-box-content">
                                    <span class="info-box-number">Precio mínimo</span>
                                    @if (Model.Moneda != "Colones")
                                    {
                                        <span class="info-box-text"><span>$</span> @precioMinFormat</span>
                                    }
                                    else
                                    {

                                        <span class="info-box-text"><span>₡</span> @precioMinFormat</span>
                                    }
                                </div>
                                <!-- /.info-box-content -->
                            </div>
                            <!-- /.info-box -->
                        </div>
                        <div class="col-md-3 col-sm-6 col-12">
