
@model Modelos.ViewModels.Propiedades.VerPropiedadViewModel

@{
    ViewData["Title"] = "Propiedades";
    var precioMaxFormat = string.Format("{0,12:0,000.00}", Model.PrecioMaximo);
    var precioMinFormat = string.Format("{0,12:0,000.00}", Model.PrecioMinimo);
    string medidaFormat = Convert.ToString(Model.TotalMedida).Replace(",00", "");

} 

<div class="preloader">
    <img src="~/ContentCliente/assets/images/favicons/logoico-02.png" class="preloader__image" alt="">
</div><!-- /.preloader -->

<section class="container-fluid content m-3">

    <input type="hidden" asp-for="IdPropiedad" id="idPropiedad" />
    <div class="row">
        <div class="col-lg-3 col-6">
            <!-- small box -->
            <div class="small-box bg-warning">
                <div class="inner">
                    <p>CÓDIGO PROPIEDAD</p>
                    <h3>@Html.DisplayFor(model => model.CodigoTipoUsoPropiedad)</h3>
                    <p>ID: @Html.DisplayFor(model => model.IdPropiedad)</p>
                </div>
                <div class="icon">
                    <i class="ion ion-ios-home"></i>
                </div>
            </div>
        </div>
        <div class="col-lg-9 col-6" style="text-align:end; align-self:center;">
            <a href="/Admin/Vendedor/VerRegistroVendedor/@Model.IdClienteVendedor"
               class="btn btn-info" data-dismiss="modal"><i class="fas fa-backward"> </i> Volver </a>
        </div>
    </div>
    <div class="card">
        <div class="card-header">
            <h3 class="card-title">Información de la propiedad (@Html.DisplayFor(model => model.CodigoTipoUsoPropiedad))</h3>
        </div>
        <div class="card-body">
            <section class="content">
                <div class="container-fluid">
                    <div class="row">
                        <div class="callout callout-danger col-md-4">
                            <h5>Estado</h5>
                            <div class="form-group">
                                <div class="custom-control custom-switch custom-switch-off-danger custom-switch-on-success">
                                    <input type="checkbox" class="custom-control-input" id="customSwitch3" asp-for="Estado">
                                    <label class="custom-control-label" for="customSwitch3"> </label>
                                </div>
                            </div>
                            <h5> Publicado </h5>
                            <div class="form-group">
                                <div class="custom-control custom-switch custom-switch-off-danger custom-switch-on-success">
                                    <input type="checkbox" class="custom-control-input" id="customSwitch4" asp-for="Publicado">
                                    <label class="custom-control-label" for="customSwitch4"> </label>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-8">
                            <div class="callout callout-danger">

                                <div class="row">

                                    <div class="col-md-4" >
                                        
                                        <a href="/Admin/Propiedad/EditarPropiedad/?id=@Html.DisplayFor(model => model.IdPropiedad)"
                                           class="btn btn-success form-control text-white" style="text-decoration:none"><span class="info-box-icon"><i class="fas fa-edit"></i></span> Editar</a>
                                    </div>
                                    <div class="col-md-4">
                                        
                                        <a href="/Admin/Propiedad/Galeria/?id=@Html.DisplayFor(model => model.IdPropiedad)"
                                           class="btn btn-success form-control text-white" style="text-decoration:none"> <span class="info-box-icon"><i class="fas fa-camera"></i></span> Galería</a>
                                    </div>
                                    <div class="col-md-4">
                                        
                                        <a href="/Admin/Propiedad/Bitacora"
                                           class="btn btn-success form-control text-white" style="text-decoration:none"><span class="info-box-icon"><i class="fas fa-list-alt"></i></span> Bitácora</a>
                                    </div>

                                </div>

                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-6 col-sm-6 col-12">
                            <div class="info-box bg-gradient-info">
                                <span class="info-box-icon"><i class="fas fa-map-marked-alt"></i></span>

                                <div class="info-box-content">
                                    <span class="info-box-number">Dirección</span>
                                    <span class="info-box-text">@Html.DisplayFor(model => model.DireccionExacta)</span>

                                    <div class="progress">
                                        <div class="progress-bar" style="width: 100%"></div>
                                    </div>
                                    <span class="progress-description">
                                        @Html.DisplayFor(model => model.Provincia), @Html.DisplayFor(model => model.Canton), @Html.DisplayFor(model => model.Distrito)
                                    </span>
                                    @{
                                        if (!string.IsNullOrEmpty(Model.BarrioPoblado))
                                        {
                                            <span class="progress-description">
                                                @Html.DisplayFor(model => model.BarrioPoblado)
                                            </span>
                                        }
                                    }
                                </div>
                                <!-- /.info-box-content -->
                            </div>
                            <!-- /.info-box -->
                        </div>
                        <!-- /.col -->
                        <div class="col-md-3 col-sm-6 col-12">
                            <div class="info-box bg-gradient-success">
                                <span class="info-box-icon"><i class="far fa-thumbs-up"></i></span>

                                <div class="info-box-content">
                                    <span class="info-box-number">Cantidad Me Gusta</span>
                                    <span class="info-box-text"> @Html.DisplayFor(model => model.TotalMeGusta) </span>

                                    <div class="progress">
                                        <div class="progress-bar" style="width: 100%"></div>
                                    </div>
                                </div>
                                <!-- /.info-box-content -->
                            </div>
                            <!-- /.info-box -->
                        </div>
                        <!-- /.col -->
                        <div class="col-md-3 col-sm-6 col-12">
                            <div class="info-box bg-gradient-warning">
                                <span class="info-box-icon"><i class="far fa-calendar-alt"></i></span>

                                <div class="info-box-content">
                                    <span class="info-box-number">Fecha registro</span>
                                    <span class="info-box-text">@Html.DisplayFor(model => model.FechaRegistra)</span>

                                    <div class="progress">
                                        <div class="progress-bar" style="width: 100%"></div>
                                    </div>
                                </div>
                                <!-- /.info-box-content -->
                            </div>
                            <!-- /.info-box -->
                        </div>
                        <!-- /.col -->
                        <div class="col-md-3 col-sm-6 col-12">
                            <div class="info-box bg-gradient-danger">
                                <span class="info-box-icon"><i class="fas fa-comments"></i></span>

                                <div class="info-box-content">
                                    <span class="info-box-number">Intención</span>
                                    <span class="info-box-text"
                                          >@Html.DisplayFor(model => model.Intencion)</span>

                                    <div class="progress">
                                        <div class="progress-bar" style="width: 100%"></div>
                                    </div>
                                </div>
                                <!-- /.info-box-content -->
                            </div>
                            <!-- /.info-box -->
                        </div>
                        <!-- /.col -->
                    </div>

                    <div class="row">
