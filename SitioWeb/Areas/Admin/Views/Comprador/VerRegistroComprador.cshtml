@* MODAL PARA AGREGAR CARACTERISTICAS *@
<div class="modal fade" id="modal-lg-agregar-caracteristicas" data-backdrop="static" data-keyboard="false" href="#">
    <div class="modal-dialog modal-lg">
        <div class="modal-content ">
            <div class="modal-header">
                <h4 class="modal-title"> <i class="fas fa-spinner fa-pulse nav-icon mr-1"></i> Agregar datos</h4>
                <button type="button" id="cerrarModalAgregarCaracteristicas" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true" class="btn btn-danger">&times;</span>
                </button>
            </div>
            <div class="modal-body ">
                @* Contenido del modal *@
                <section class="content container">

                    <div class="container">
                        <!-- APPLICATION -->
                        <div id="App">

                            <div class="row">
                                <!-- left column -->
                                <div class="col-md-12">
                                    <!-- general form elements -->
                                    <div class="card card-primary">

                                        <div class="card-body">
                                            <div class="card">
                                                <form id="formularioCaracteristicas" class="m-2 text-center">
                                                    <div class="row">
                                                        <div class="col-md-6">
                                                            <div class="form-group">
                                                                <label for="inputStatus"> Tipo de propiedad </label>
                                                                <select id="tipoPropiedad" name="tipoPropiedad" class="form-control"></select>
                                                                <span id="ErrorTipoPropiedad" class="text-danger"></span>
                                                            </div>
                                                        </div>
                                                        <div class="col-md-6">
                                                            <div class="form-group">
                                                                <label for="inputStatus"> Lugar donde desea comprar</label>
                                                                <input type="text" id="lugarCompra" placeholder="Lugar"
                                                                       class="form-control">
                                                                <span id="ErrorLugarCompra" class="text-danger"></span>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        <div class="col-md-6">
                                                            <div class="form-group">
                                                                <div class="form-check">
                                                                    <input type="checkbox" class="form-check-input" value="S" name="CheckPoseePropiedad" id="CheckPoseePropiedad">
                                                                    <label class="text-black">¿Posee propiedad específica?</label>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-md-6">
                                                            <div class="form-group">
                                                                <label for="inputStatus">Presupuesto</label>
                                                                <input type="number" name="name" id="presupuesto" class="form-control" value="0" min="0" step="10000" style="width:100%;" />
                                                                <span id="ErrorPresupuesto" class="text-danger"></span>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="row m-2">
                                                        <div class="col-md-12">
                                                            <div class="container-fluid mt-2 bg-white">
                                                                <div class="row mb-2">
                                                                    <div class="col-12 text-center">
                                                                        <h3 class="text-black" id="tituloSeleccionePropiedad"> Seleccione la propiedad </h3>
                                                                        <h5 class="text-black" id="codigoPropiedadElegido"></h5>
                                                                        <h5 class="text-danger" id="ErrorCodigoPropiedad"></h5>
                                                                    </div>
                                                                </div>

                                                                <table id="tblPropiedades" class="table table-hover table-bordered table-responsive table-striped mt-2" style="width:100%;">
                                                                    <thead>
                                                                        <tr>
                                                                            <th>Id</th>
                                                                            <th>Código</th>
                                                                            <th>Tipo </th>
                                                                            <th>Dueño</th>
                                                                            <th>Uso</th>
                                                                            <th>Ubicación</th>
                                                                            <th>Medida</th>
                                                                            <th>Precio Máx.</th>
                                                                            <th>Precio Mín.</th>
                                                                            <th>Topografía</th>
                                                                            <th>Publicación</th>
                                                                            <th>Acción</th>
                                                                        </tr>
                                                                    </thead>
                                                                </table>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="row align-content-center">
                                                        <div class="col-md-12">
                                                            <button type="submit" id="SubmitBtnAgregarCaract" class="btn btn-success"><i class="fas fa-check-square"> Guardar   </i></button>
                                                        </div>
                                                    </div>
                                                </form>
                                            </div>
                                        </div>

                                    </div>
                                    <!-- /.card -->
                                </div>
                            </div>
                        </div>
                    </div>

                </section>
                <div class="modal-footer">
                    <button type="button" id="btnCancelarAgregarDivision" class="btn btn-warning" data-dismiss="modal">Cancelar</button>
                </div>
            </div>
        </div>
        <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
</div>

