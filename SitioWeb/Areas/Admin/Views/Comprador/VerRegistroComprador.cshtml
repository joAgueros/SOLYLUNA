@model Modelos.ViewModels.Clientes.CompradorPropiedadViewModel

<div class="preloader">
    <img src="~/ContentCliente/assets/images/loader.png" class="preloader__image" alt="">
</div><!-- /.preloader -->

<section class="content mt-2">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-3">
                <input type="hidden" asp-for="Comprador.IdClienteComprador" id="idClienteComprador" />
                <!-- Profile Image -->
                <div class="card card-primary card-outline">
                    <div class="card-body box-profile">
                        <div class="text-center">
                            <img class="profile-user-img img-fluid img-circle" src="~/img/usuario.png" alt="User profile picture">
                        </div>

                        <h3 class="profile-username text-center">@Html.DisplayFor(model => model.Comprador.NombreCompleto)</h3>
                        <a href="/Admin/Comprador/Editar/@Html.DisplayFor(model => model.Comprador.IdClienteComprador)" class="btn btn-primary btn-block"><b><i class="fas fa-pen-alt mr-1"></i>Editar</b></a>
                        <hr />
                        <div class="card card-primary">
                            <div class="card-header">
                                <h3 class="card-title">Datos del Comprador</h3>
                            </div>
                            <!-- /.card-header -->
                            <div class="card-body">
                                <strong><i class="fas fa-envelope-open mr-1"></i> Correo electrónico </strong>

                                <p class="text-muted">
                                    @Html.DisplayFor(model => model.Comprador.CorreoElectronico)
                                </p>

                                <hr>

                                <strong><i class="fas fa-map-marker-alt mr-1"></i> Ubicación </strong>

                                <p class="text-muted">
                                    @Html.DisplayFor(model => model.Comprador.Provincia),
                                    @Html.DisplayFor(model => model.Comprador.Canton),
                                    @Html.DisplayFor(model => model.Comprador.Distrito)
                                </p>

                                <hr>

                                <strong><i class="fas fa-address-book mr-1"></i> Dirección exacta </strong>

                                <p class="text-muted">
                                    @Html.DisplayFor(model => model.Comprador.DireccionExacta)
                                </p>

                                <hr>

                                <strong><i class="fas fa-phone-alt mr-1"></i> Teléfonos </strong>
                                <hr />
                                <strong> Celular </strong>
                                <p class="text-muted">
                                    @Html.DisplayFor(model => model.Comprador.TelefonoMovil)
                                </p>
                                <strong> Casa </strong>
                                <p class="text-muted">
                                    @Html.DisplayFor(model => model.Comprador.TelefonoCasa)
                                </p>
                                <strong> Oficina </strong>
                                <p class="text-muted">
                                    @Html.DisplayFor(model => model.Comprador.TelefonoOficina)
                                </p>

                                <hr>

                                <strong><i class="far fa-file-alt mr-1"></i> Identificación </strong>

                                <hr />
                                <strong> Identificación </strong>
                                <p class="text-muted">
                                    @Html.DisplayFor(model => model.Comprador.Identificacion)
                                </p>
                                <strong> Tipo Identificación </strong>
                                <p class="text-muted">
                                    @Html.DisplayFor(model => model.Comprador.TipoIdentificacion)
                                </p>
                                <strong> Persona Jurídica </strong>
                                <p class="text-muted">
                                    @Html.DisplayFor(model => model.Comprador.PersonaJuridica)
                                </p>

                            </div>
                            <!-- /.card-body -->
                        </div>

                    </div>
                    <!-- /.card-body -->
                </div>
                <!-- /.card -->
            </div>
            <!-- /.col -->
            <div class="col-md-9">
                <!-- general form elements disabled -->
                <div class="card card-default">
                   
                    <div class="card-header ">
                        <h3 class="float-left"> <i class="fa fa-home nav-icon"></i> Referencias</h3>
                        <a style="color:white" href="/Admin/Comprador/"
