                                                        <span id="ErrorObservacionEntubadoEdit" class="text-danger"></span>
                                                    </div>
                                                </div>
                                            </div>

                                            <!-- /.card-body -->
                                        </div>
                                    </form>
                                    <!-- /.card -->
                                </div>

                            </div>
                        </div>
                    </div>

                </section>
                <div class="modal-footer">
                    <input type="button" value="Cancelar" id="btnCancelarEditarTipoCableado" data-dismiss="modal" class="btn btn-warning">
                    <input type="button" value="Guardar" id="btnGuardarTiposCableadoEdit" class="btn btn-success">
                </div>
            </div>
        </div>
        <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
</div>

@* MODAL PARA AGREGAR TIPOS DE CABLEADO A LA CONSTRUCCION *@
<div class="modal fade" id="modal-lg-agregar-tipos-cableado" data-backdrop="static" data-keyboard="false" href="#">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title"> <i class="fas fa-spinner fa-pulse nav-icon mr-1"></i>  Agregar tipos de cableado</h4>
                <button type="button" id="cerrarModalAgregarCableado" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true" class="btn btn-danger">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                @* Contenido del modal *@
                <section class="content container">

                    <div class="container">
                        <!-- APPLICATION -->
                        <div id="App">

                            <div class="row">
                                <div class="col-md-12">
                                    <!-- Form Element sizes -->
                                    <form id="formTipoCableado">
                                        <div class="card card-success">

