    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title"> <i class="fas fa-spinner fa-pulse nav-icon mr-1"></i>  Editar tipo de cableado</h4>
                <button type="button" id="cerrarModalEditarTipoCableado" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true" class="btn btn-danger">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                @* Contenido del modal *@
                <section class="content container">

                    <div class="container">
                        <!-- APPLICATION -->
                        <div id="App">

                            <div class="row">
                                <div class="col-md-12">
                                    <!-- Form Element sizes -->
                                    <form id="formTipoCableadoEditar">
                                        <div class="card card-success">

                                            <div class="row">

                                                <div class="col-md-12 m-3">

                                                    <h3 id="nombreTipoCableado"></h3>
